{
    "version": 2,
    "builds": [
        {
            "src": "api",
            "use": "@vercel/node"
        },
        {
            "src": "front-work",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api(/.*)",
            "dest": "/api/server.js"
        },
        {
            "src": "/",
            "dest": "/front-work/src/pages/index.tsx"
        }
    ]
}